<Application x:Class="InoriDock.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:InoriDock.WPF"
             StartupUri="/Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/WindowStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Components/DockComponent/Style/DockItemStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Components/DockComponent/Animation/DockItemAnimation.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
